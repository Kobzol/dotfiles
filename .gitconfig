[credential]
        helper = store
[pull]
        ff = only
[commit]
        gpgsign = true
[alias]
        uncommit = reset --soft HEAD^1
[alias]
        squash = "!f() { git reset --soft \"$(git merge-base HEAD master)\"; }; f"
[alias]
	co = checkout
[rerere]
        enabled = true
[rebase]
        autosquash = true
[help]
	autocorrect = prompt
